@model IntroToMVC5.Models.Authors
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<h3>@ViewBag.Greetings</h3>
<h4>@ViewBag.Name</h4>
<h4>@ViewBag.Country</h4>

<h4>@Model.AuthorName</h4>
<h4>@Model.AuthorCountry</h4>

<form action="/HelloWorld/gotoUrl" method="GET">
    <label for="url">URL:</label>
    <input type="text" name="url" value="" />
    <br />
    <input type="submit" name="name" value="Sabmit" />
</form>
